{
  "name": "rails-request",
  "version": "0.0.2",
  "description": "Utility for producing a Rails-compatible object from a JavaScript or JSON one; perfect for create (POST) or update (PUT) endpoints",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel --presets=es2015,stage-0 src -d lib",
    "build-for-tests": "babel --presets=es2015,stage-0 src -d lib --source-maps",
    "lint": "./node_modules/.bin/eslint ./src",
    "build-tests": "babel --presets=es2015,stage-0 spec -d lib/spec --source-maps",
    "tests": "npm run build-for-tests && npm run build-tests && jasmine",
    "watch-tests": "nodemon --exec 'rm -rf lib && npm run tests || true'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greena13/rails-request.git"
  },
  "keywords": [
    "update",
    "diff",
    "difference",
    "Rails"
  ],
  "files": [
    "lib",
    "package.json",
    "README.md"
  ],
  "devDependencies": {
    "babel-cli": "^6.0.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "eslint": "^3.14.1",
    "eslint-plugin-import": "^2.2.0",
    "jasmine": "^2.5.3",
    "nodemon": "^1.11.0"
  },
  "author": "Aleck Greenham",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/greena13/rails-request/issues"
  },
  "homepage": "https://github.com/greena13/rails-request#readme",
  "dependencies": {
    "lodash.camelcase": "^4.3.0",
    "lodash.endswith": "^4.2.1",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.has": "^4.5.2",
    "lodash.isplainobject": "^4.0.6",
    "lodash.isundefined": "^3.0.1",
    "lodash.map": "^4.6.0",
    "lodash.reduce": "^4.6.0",
    "lodash.size": "^4.2.0",
    "lodash.snakecase": "^4.1.1",
    "lodash.some": "^4.6.0",
    "lodash.without": "^4.4.0"
  }
}
